{
    "extends": "tslint-config-airbnb-base",
    "rules":{
        "no-console": "false",
        "no-plusplus": ["error",{ "allowForLoopAfterthoughts": true }],
        "comma-dangle": ["error", {
            "functions": "never",
            "objects": "always-multiline",
            "arrays": "always-multiline"
        }],
        "no-unused-vars": ["error", {"varsIgnorePattern": "_" }]
        
    },
      "jsRules": {
    "no-console": false 
  }
}
