{
    "extends": "tslint-config-airbnb-base",
    "rules":{
        "no-underscore-dangle": 0,
        "no-plusplus": ["error",{ "allowForLoopAfterthoughts": true }],
        "comma-dangle": ["error", {
            "functions": "never",
            "objects": "always-multiline",
            "arrays": "always-multiline"
        }],
        "no-unused-vars": ["error", {"varsIgnorePattern": "_" }],
        "no-else-return": "off"
        
    }
}
